<?xml  version="1.0"?>
<robot name ="dual_arm_agv"> 
    <!-- Author: Chengjin Wang; e-mail:2210991@tongji.edu.cn. The urdf file was develped in tongji university.-->

    <!--create  color params-->
    <material name ="LightGrey">
        <color rgba="0.7 0.7 0.7 1.0"/>
    </material>

    <!--create mass params
    <mass name ="agv_base_mass">
        <mass value="1.0"/>
    </mass>

    <mass name ="shoulder_mass">
        <mass value="2.0"/>
    </mass>
    <mass name ="upper_arm_mass">
        <mass value="3.42"/>
    </mass>
    <mass name ="forearm_mass">
        <mass value="1.26"/>
    </mass>
    <mass name ="wrist_1_mass">
        <mass value="0.8"/>
    </mass>
    <mass name ="wrist_2_mass">
        <mass value="0.8"/>
    </mass>
    <mass name ="wrist_3_mass">
        <mass value="0.35"/>
    </mass>
-->

    <!--Create a world.-->
    <link  name ="World"/>

    


    <!--arm base link to agv base joint-->
    <joint name="right_base_link_to_world" type="fixed">
        <parent link="World"/>
        <child link="right_base_link"/>
        <axis xyz="0.0 0.0 1.0"/>
        <origin xyz="0 0. 1" rpy="-1.5708 -0.7854 0.0" />
        <limit lower="-6.28318530718" upper="6.28318530718" effort="56.0" velocity="3.14159265359"/>
    </joint>

    <!--link chain: arm base link -shouder link- upper link- forearm link- wrist link 1 - wrist link 2 - wrist link3 -->
    <!-- right arm base link-->
    <link name="right_base_link">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename ="meshes/base.dae"/>
            </geometry>
            <material name = "LightGrey"/>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                 <mesh filename = "meshes/base.stl"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="2.0"/>
            <inertia ixx="0.003053165" ixy="0.0" ixz="0.0" iyy="0.003053165" iyz="0.0" izz="0.005625"/>
        </inertial>
    </link>

    
    <!--right arm joint 1 -->
    <joint name="right_shoulder_link_to_right_base_link" type="revolute">
            <origin xyz="0.0 0.0 0.1519 " rpy="0.0 0.0 0.0"/>
            <parent link="right_base_link"/>
            <child link="right_shoulder_link"/>
            <axis xyz="0.0 0.0 1.0"/>
            <limit lower="-6.28318530718" upper="6.28318530718" effort="56.0" velocity="1.5708"/>
    </joint>

    <!--right shouder link-->
  
    <link name="right_shoulder_link">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 3.14159"/>
            <geometry>
                <mesh filename ="meshes/shoulder.dae"/>
            </geometry>
            <material name = "LightGrey"/>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 3.14159"/>
            <geometry>
                    <mesh filename ="meshes/shoulder.stl"/>
            </geometry>
        </collision>
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value ="2.0"/>
            <inertia ixx="0.008093163" ixy="0.0" ixz="0.0" iyy="0.008093163" iyz="0.0" izz="0.005625"/>
        </inertial>
    </link>

    <!--right arm joint 2-->
    <joint name="right_upper_arm_link_to_right_shoulder_link" type="revolute">
        <origin xyz="0.0 0.0 0.0" rpy="1.5708 0.0  0."/>
        <parent link="right_shoulder_link"/>
        <child link="right_upper_arm_link"/>
        <axis xyz="0.0 0.0 1"/>
        <limit lower="-6.28318530718" upper="6.28318530718" effort="56.0" velocity="1.5708"/>
    </joint>

    <!--right upper link -->
    <link name="right_upper_arm_link">
        <inertial>
            <origin xyz="-0.121825 0.0 0.12" rpy="  0.0 1.5708 0.0"/>
            <mass value ="3.42"/>
            <inertia ixx="0.021728483" ixy="0.0" ixz="0.0" iyy="0.021728483" iyz="0.0" izz="0.00961875"/>
        </inertial>
        <visual >
            <origin xyz="0.0 0.0 0.1198" rpy="1.5708 0.0 -1.5708 "/>
            <geometry>
                <mesh filename="meshes/upperarm.dae"/>
            </geometry>
            <material name="LightGrey"/>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.1198" rpy="1.5708 0.0 -1.5708"/>
            <geometry>
                <mesh filename="meshes/upperarm.stl"/>
            </geometry>
        </collision>
    </link>


    <!--right arm joint 3-->
    <joint name="right_forearm_link_to_right_upper_arm_link" type="revolute">
        <origin xyz="-0.24355 0.0 0.0" rpy="0.0 0.0 0.0"/>
        <parent link="right_upper_arm_link"/>
        <child link="right_forearm_link"/>
        <axis xyz="0.0 0.0 1"/>
        <limit lower="-6.28318530718" upper="6.28318530718" effort="28.0" velocity="1.5708"/>
    </joint>

    <!---->
    <link name="right_forearm_link">
        <inertial>
            <origin xyz="-0.121825 0.0 0.027" rpy=" 0.0  1.5708 0.0"/>
            <mass value  = "1.26"/>
            <inertia ixx="0.006544568" ixy="0.0" ixz="0.0" iyy="0.006544568" iyz="0.0" izz="0.000354375"/>
        </inertial>
        <visual >
            <origin xyz="0.0 0.0 0.0275" rpy="1.5708 0.0 -1.5708"/>
            <geometry>
                <mesh filename="meshes/forearm.dae"/>
            </geometry>
            <material  name ="LightGrey"/>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0275" rpy="1.5708 0.0 -1.5708"/>
            <geometry>
                <mesh filename="meshes/forearm.stl"/>
            </geometry>
        </collision>
    </link>

    <!--right wrist1_joint--> 
    <joint name="right_wrist_1_link_to_right_forearm_link" type="revolute">
        <origin xyz="-0.21325 0.0 0.10305" rpy="0.0 0.0 0.0"/>
        <parent link="right_forearm_link"/>
        <child link="right_wrist_1_link"/>
        <axis xyz="0.0 0.0 1"/>
        <limit lower="-6.28318530718" upper="6.28318530718" effort="12.0" velocity="6.28318530718"/>
    </joint>

    <!---->
    <link name="right_wrist_1_link">
        <inertial>
            <origin xyz=" 0.0  0.0  0.0" rpy="0.0  0.0 0.0"/>
            <mass value  ="0.8"/>
            <inertia ixx="0.002084999" ixy="0.0" ixz="0.0" iyy="0.002084999" iyz="0.0" izz="0.00225"/>
        </inertial>
        <visual>
            <origin xyz="0.0 0.0 -0.10305" rpy="1.5708 0.0 0.0"/>
            <geometry>
            <mesh filename="meshes/wrist1.dae"/>
            </geometry>
            <material  name ="LightGrey"/>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 -0.10305" rpy="1.5708 0.0 0.0"/>
            <geometry>
                <mesh filename="meshes/wrist1.stl"/>
            </geometry>
        </collision>
    </link>

    <!--right wrist2_joint -->
    <joint name="right_wrist_2_link_to_right_wrist_1_link" type="revolute">
        <origin xyz="0.0 -0.08535 0.0" rpy="1.5708 0.0 0.0"/>
        <parent link="right_wrist_1_link"/>
        <child link="right_wrist_2_link"/>
        <axis xyz="0.0 0.0 1"/>
        <limit lower="-6.28318530718" upper="6.28318530718" effort="12.0" velocity="6.28318530718"/> 
    </joint>

    <!---->
    <link name="right_wrist_2_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value  = "0.8"/>
            <inertia ixx="0.002084999" ixy="0.0" ixz="0.0" iyy="0.002084999" iyz="0.0" izz="0.00225"/>
        </inertial>
        <visual >
            <origin xyz="0.0 0.0 -0.08535" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="meshes/wrist2.dae"/>
            </geometry>
            <material  name ="LightGrey"/>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 -0.08535" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="meshes/wrist2.stl"/>
            </geometry>
        </collision>
    </link>
    <!--right wrist3_joint-->
    <joint name="right_wrist_3_link_to_right_wrist_2_link" type="revolute">
        <origin xyz="0.0 0.0921 0.0" rpy="-1.5708 0.0 0.0"/>
        <parent link="right_wrist_2_link"/>
        <child link="right_wrist_3_link"/>
        <axis xyz="0.0 0.0 1"/>
        <limit lower="-6.28318530718" upper="6.28318530718" effort="12.0" velocity="6.28318530718"/>
    </joint>

    <!--3-->
    <link name="right_wrist_3_link">
        <inertial>
            <origin xyz="0.0 0.0 -0.0175" rpy="0.0 0.0 0.0"/>
            <mass value  ="0.35"/>
            <inertia ixx="0.000136267" ixy="0.0" ixz="0.0" iyy="0.000136267" iyz="0.0" izz="0.0004096"/>
        </inertial>
        <visual >
            <origin xyz="0.0 0.0 -0.0921" rpy="1.5708 0.0 0.0"/>
            <geometry>
                <mesh filename="meshes/wrist3.stl"/>
            </geometry>
            <material  name ="LightGrey"/>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 -0.0921" rpy="1.5708 0.0 0.0"/>
            <geometry>
                <mesh filename="meshes/wrist3.stl"/>
            </geometry>
        </collision>
    </link>
</robot>
